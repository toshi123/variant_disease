   "source": [
    "#acをとってくるコード\n",
    "data = open(\"../uniprot_sprot_human.dat\")\n",
    "\n",
    "acs=[]\n",
    "for line in data:\n",
    "    if line.startswith(\"AC\"):\n",
    "        info = line.rstrip().split()\n",
    "        # ACが複数あるときに対応\n",
    "        for a in info[1:]:\n",
    "            acs.append(a.replace(\";\",\"\"))            \n",
    "\n",
    "#全てのACがリストで出力される\n",
    "print(acs)"
   ]
